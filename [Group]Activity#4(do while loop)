#include <iostream>
#include <string>
using namespace std;

int main() {
    string password = "open123";  
    string userInput;

    do {
        cout << "Enter password: ";
        cin >> userInput;

        if (userInput != password) {
            cout << "Incorrect password. Try again." << endl;
        }

    } while (userInput != password);

    cout << "Access granted. Welcome!" << endl;

    return 0;
}
