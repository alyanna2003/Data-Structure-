#include <iostream>
using namespace std;

int main() {
    int secretNumber = 7;  
    int guess;

    cout << "Guess the secret number between 1 and 10: ";
    cin >> guess;

    while (guess != secretNumber) {
        cout << "Wrong! Try again: ";
        cin >> guess;
    }

    cout << "Congratulations! You guessed the secret number: " << secretNumber << endl;

    return 0;
}
